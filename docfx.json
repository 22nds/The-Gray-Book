{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "introduction/**.md",
          "introduction/**/toc.md",
		      "reference/**.md",
          "reference/**/toc.md",
          "changelog/**.md",
          "changelog/**/toc.md",
          "roadmap/**.md",
          "roadmap/**/toc.md",
          "toc.md",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "globalMetadata": {
      "_improveThisDoc": "Edit this page",
      "_inThisArticle": "IN THIS ARTICLE",
      "_appTitle": "vvvv gamma documentation",
      "_appFooter": "",
      "_enableSearch": false,
      "_enableNewTab": true,
      "_appLogoPath": "/images/logo.svg",
      "_appFaviconPath": "/images/favicon.ico",
      "_disableContribution": false,
      "_disableSideFilter": true,
      "_disableAffix": false,
      "_gitContribute": {
        "repo": "https://github.com/vvvv/The-Gray-Book.git",
        "branch": "master"
      },
      "_gitUrlPattern": "github"
    },
	"template": [
        "default",
        "modern",
        "custom template/vvvv_modern"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "markdownEngineProperties": {
      "toc": {
        "levels": "1,2,3"
      }
    },
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
